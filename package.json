{
  "name": "blueprint-api-server",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "npx tsc",
    "start": "concurrently \"nodemon dist/app.js\" \"npx tsc --watch\"",
    "migrate": "DATABASE_URL=postgresql://postgres:postgres@localhost:5432 node ./node_modules/.bin/node-pg-migrate",
    "seed": "DATABASE_URL=postgresql://postgres:postgres@localhost:5432 node --unhandled-rejections=strict scripts/seed_blueprint_screener.js",
    "test": "jest"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "~4.16.1",
    "helmet": "^6.0.0",
    "node-pg-migrate": "^6.2.2",
    "npm": "^8.19.1",
    "pg": "^8.8.0",
    "squid": "^0.5.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.1.7",
    "concurrently": "^8.0.1",
    "jest": "^29.0.3",
    "nodemon": "^2.0.22",
    "typescript": "^5.0.4"
  }
}
